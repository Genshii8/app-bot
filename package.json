{
  "name": "app-bot",
  "version": "0.1.0",
  "repository": "https://github.com/Genshii8/app-bot",
  "author": "Genshii8",
  "license": "MIT",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "type": "module",
  "scripts": {
    "start": "yarn build && pm2 start ecosystem.config.js --only app-bot",
    "test": "yarn build && pm2 start ecosystem.config.js --only test-app-bot",
    "restart": "yarn build && pm2 restart app-bot",
    "retest": "yarn build && pm2 restart test-app-bot",
    "build": "tsc",
    "lint": "xo",
    "fix": "xo --fix"
  },
  "dependencies": {
    "discord.js": "^12.5.3",
    "dotenv": "^9.0.2",
    "node-persist": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^15.0.3",
    "@types/node-persist": "^3.1.1",
    "@types/ws": "^7.4.4",
    "typescript": "^4.2.4",
    "xo": "^0.40.1"
  },
  "xo": {
    "semicolon": false,
    "prettier": true
  }
}
