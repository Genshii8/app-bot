{
  "name": "discord-app-bot",
  "version": "0.1.0",
  "type": "module",
  "description": "A Discord bot for automating the handling of Google Forms pushed to a server.",
  "repository": "github:adamhl8/discord-app-bot",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "rm -rf dist && pnpm lint && tsc && pnpm format",
    "docker:push": "pnpm build && docker build . -t ghcr.io/adamhl8/discord-app-bot && docker push ghcr.io/adamhl8/discord-app-bot:latest",
    "format": "prettier --write .",
    "lint": "eslint -f pretty .",
    "start": "ts-node ./src/index.ts"
  },
  "dependencies": {
    "@discordjs/rest": "^1.5.0",
    "@sindresorhus/slugify": "^2.1.1",
    "discord-api-types": "^0.37.23",
    "discord-bot-shared": "^0.9.1",
    "discord.js": "^14.7.1",
    "fs-extra": "^11.1.0",
    "get-urls": "^11.0.0",
    "node-json-db": "^2.1.3"
  },
  "devDependencies": {
    "@swc/core": "^1.3.23",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-sonarjs": "^0.17.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "prettier": "^2.8.1",
    "prettier-plugin-organize-imports": "^3.2.1",
    "prettier-plugin-pkg": "^0.17.1",
    "prettier-plugin-sh": "^0.12.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
